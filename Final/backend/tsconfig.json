{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "strict": false,
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true,
      
   }
}
